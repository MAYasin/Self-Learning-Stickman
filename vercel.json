{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["/**"] }
        },
        {
          "src": "src/**",
          "use": "@vercel/static",
          "config": { "includeFiles": ["src/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}